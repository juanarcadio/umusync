<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
 <converter>
  <converter-id>CriteriaConverter</converter-id>
  <converter-class>umu.sakai.umusync.tool.converters.CriteriaConverter</converter-class>
 </converter>
 <converter>
  <converter-id>IgnoreFunctionsModeConverter</converter-id>
  <converter-class>umu.sakai.umusync.tool.converters.IgnoreFunctionsModeConverter</converter-class>
 </converter>
 <converter>
  <converter-id>HomePageConverter</converter-id>
  <converter-class>umu.sakai.umusync.tool.converters.HomePageConverter</converter-class>
 </converter>
 <converter>
  <converter-id>SiteTypeConverter</converter-id>
  <converter-class>umu.sakai.umusync.tool.converters.SiteTypeConverter</converter-class>
 </converter>
 <managed-bean>
  <description>Dynamic Resource Bundle Loader</description>
  <managed-bean-name>umumessages</managed-bean-name>
  <managed-bean-class>org.sakaiproject.util.ResourceLoader</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <description>Bundle baseName</description>
   <property-name>baseName</property-name>
   <value>umu.sakai.umusync.tool.bundle.Messages</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>SyncBean</managed-bean-name>
  <managed-bean-class>umu.sakai.umusync.tool.SyncBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>services</property-name>
   <value>#{umusyncServices}</value>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/umusync/tasklist.jsp</from-view-id>
  <navigation-case>
   <from-outcome>edit</from-outcome>
   <to-view-id>/umusync/taskedit.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>exec</from-outcome>
   <to-view-id>/umusync/taskexecute.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>page</from-outcome>
   <to-view-id>/umusync/pagelist.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>criteria</from-outcome>
   <to-view-id>/umusync/criterialist.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/umusync/criterialist.jsp</from-view-id>
  <navigation-case>
   <from-outcome>edit</from-outcome>
   <to-view-id>/umusync/criteriaedit.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>task</from-outcome>
   <to-view-id>/umusync/tasklist.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>page</from-outcome>
   <to-view-id>/umusync/pagelist.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/umusync/pagelist.jsp</from-view-id>
  <navigation-case>
   <from-outcome>edit</from-outcome>
   <to-view-id>/umusync/pageedit.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>criteria</from-outcome>
   <to-view-id>/umusync/criterialist.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>task</from-outcome>
   <to-view-id>/umusync/tasklist.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/umusync/taskedit.jsp</from-view-id>
  <navigation-case>
   <from-outcome>task</from-outcome>
   <to-view-id>/umusync/tasklist.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/umusync/criteriaedit.jsp</from-view-id>
  <navigation-case>
   <from-outcome>criteria</from-outcome>
   <to-view-id>/umusync/criterialist.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/umusync/pageedit.jsp</from-view-id>
  <navigation-case>
   <from-outcome>page</from-outcome>
   <to-view-id>/umusync/pagelist.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/umusync/taskexecute.jsp</from-view-id>
  <navigation-case>
   <from-outcome>rtn</from-outcome>
   <to-view-id>/umusync/tasklist.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <validator>
  <validator-id>SyncJpaValidator</validator-id>
  <validator-class>umu.sakai.jsf.validator.JPAValidator</validator-class>
 </validator>
 <application>
  <locale-config>
   <default-locale>es</default-locale>
   <supported-locale>en</supported-locale>
  </locale-config>
  <message-bundle>umu.sakai.umusync.tool.bundle.Messages</message-bundle>
 </application>
</faces-config>
