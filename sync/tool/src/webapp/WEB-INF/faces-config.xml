<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	version="2.0">
	<converter>
		<converter-id>CriteriaConverter</converter-id>
		<converter-class>umu.sakai.umusync.tool.converters.CriteriaConverter
		</converter-class>
	</converter>
	<converter>
		<converter-id>IgnoreFunctionsModeConverter</converter-id>
		<converter-class>umu.sakai.umusync.tool.converters.IgnoreFunctionsModeConverter
		</converter-class>
	</converter>
	<converter>
		<converter-id>HomePageConverter</converter-id>
		<converter-class>umu.sakai.umusync.tool.converters.HomePageConverter
		</converter-class>
	</converter>
	<converter>
		<converter-id>SiteTypeConverter</converter-id>
		<converter-class>umu.sakai.umusync.tool.converters.SiteTypeConverter
		</converter-class>
	</converter>
	<managed-bean>
		<description>Dynamic Resource Bundle Loader</description>
		<managed-bean-name>msgs</managed-bean-name>
		<managed-bean-class>org.sakaiproject.util.ResourceLoader</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<description>Bundle baseName</description>
			<property-name>baseName</property-name>
			<value>umu.sakai.umusync.tool.bundle.Messages</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>SyncBean</managed-bean-name>
		<managed-bean-class>umu.sakai.umusync.tool.SyncBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>services</property-name>
			<value>#{umusyncServices}</value>
		</managed-property>
	</managed-bean>	
	<navigation-rule>
		<from-view-id>/umusync/tasklist.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/umusync/taskedit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>exec</from-outcome>
			<to-view-id>/umusync/taskexecute.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>page</from-outcome>
			<to-view-id>/umusync/pagelist.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>criteria</from-outcome>
			<to-view-id>/umusync/criterialist.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/umusync/criterialist.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/umusync/criteriaedit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>task</from-outcome>
			<to-view-id>/umusync/tasklist.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>page</from-outcome>
			<to-view-id>/umusync/pagelist.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/umusync/pagelist.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/umusync/pageedit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>criteria</from-outcome>
			<to-view-id>/umusync/criterialist.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>task</from-outcome>
			<to-view-id>/umusync/tasklist.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/umusync/taskedit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>task</from-outcome>
			<to-view-id>/umusync/tasklist.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/umusync/criteriaedit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>criteria</from-outcome>
			<to-view-id>/umusync/criterialist.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/umusync/pageedit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>page</from-outcome>
			<to-view-id>/umusync/pagelist.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/umusync/taskexecute.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>rtn</from-outcome>
			<to-view-id>/umusync/tasklist.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<validator>
		<validator-id>SyncJpaValidator</validator-id>
		<validator-class>umu.sakai.umusync.tool.validator.JPAValidator
		</validator-class>
	</validator>
	<application>
		<locale-config>
			<default-locale>es</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
		<message-bundle>umu.sakai.umusync.tool.bundle.Messages
		</message-bundle>
	</application>
</faces-config>
